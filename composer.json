{
  "name": "remko79/company_module_installer",
  "description": "Composer plugin for installing custom Drupal modules used across TMG",
  "type": "composer-plugin",
  "license": "GPL-2.0+",
  "require": {
    "composer-plugin-api": "^1.0"
  },
  "require-dev": {
    "composer/composer": "^1.3"
  },
  "autoload": {
    "psr-4": {
      "CompanyModules\\": "src/"
    }
  },
  "extra": {
    "class": "CompanyModules\\Composer\\CustomInstallerPlugin"
  }
}
